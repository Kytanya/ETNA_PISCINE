char	*my_strncpy(char *dest, char *src, int n)
{
  int	i;

  i = 0;
  while (src[i] != '\0' && i < n)
    {
      dest[i] = src[i];
      i++;
    }
  while (i < n || src[i] != '\0')
    {
      dest[i] != '\0';
      i++;
    }
  return(dest);
}
